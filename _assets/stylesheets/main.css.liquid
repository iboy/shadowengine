ul.hardware-items li img {
  width: 100%;
  height: auto;
  margin: 0; }

ul.hardware-items li .detail {
  width: 181px;
  height: 36px;
  padding: 10px 3px;
  background: #438aa5;
  color: white;
  font-size: 12px;
  line-height: 18px; }

.component img {
  width: 50%; }

.content {
  margin: 0 50px; }

.content p {
  margin: 20px 0; }

.content ul, .content ol {
  margin: 20px 0;
  padding-left: 20px; }

.content li {
  margin: 20px 0; }

.content ul li {
  list-style-type: square; }

.content ol li {
  padding: 0; }

.content img {
  display: block;
  margin: 40px auto;
  max-width: 760px; }

.content pre, .content code {
  background: #f2f1f0;
  border: 1px solid #ddd;
  font-family: "Courier", monospace;
  padding: 2px 5px;
  font-size: 16px; }

.content pre {
  width: 700px;
  margin: 20px 0;
  padding: 10px;
  font-size: 14px;
  line-height: 20px;
  white-space: pre-wrap; }

.content pre code {
  background: transparent;
  border: 0;
  padding: 0;
  font-size: 14px; }

.token.operator, .token.entity, .token.url {
  background: transparent; }

.language-css .token.string, .style .token.string, .token.variable {
  background: transparent; }

.content table {
  margin: 0 auto;
  border-collapse: collapse; }

.content table thead {
  display: table-header-group;
  vertical-align: middle;
  border-color: inherit;
  border-top: 1px solid #ccc;
  background-color: #fff; }

.content table tr {
  border-top: 1px solid #ccc;
  background-color: #fff; }

.content table th {
  border: 1px solid #ddd;
  padding: 6px 13px;
  font-weight: bold; }

.content table tbody {
  display: table-row-group;
  vertical-align: middle;
  border-color: inherit; }

.content table td {
  border: 1px solid #ddd;
  padding: 6px 13px; }

.content iframe {
  margin: 0 auto;
  display: block; }

footer.footer {
  margin: 80px 50px 0;
  padding: 0 0 20px;
  height: 30px; }

footer.footer p {
  margin: 0;
  font-size: 13px;
  line-height: 18px;
  color: #555; }

footer.footer small {
  float: left;
  width: 670px;
  margin-top: -1px; }

footer.footer a.logo {
  height: 32px;
  float: left;
  display: block;
  text-indent: -9999px; }

footer.footer a.cc {
  background: url("{{ site.baseurl }}{% asset_path creative-commons.png %}");
  width: 88px;
  margin-right: 10px; }

footer.footer a.gh {
  background: url("{{ site.baseurl }}{% asset_path octocat.png %}");
  width: 32px;
  float: right; }

footer.footer a:link, footer.footer a:visited {
  color: #438aa5;
  text-decoration: none; }

footer.footer a:hover {
  text-decoration: underline; }

@media screen and (max-width: 920px) {
  .container {
    width: 100%;
    border: 0; }
  .container * {
    max-width: 100% !important; }
  .main {
    width: 100%;
    margin: 0;
    padding: 0; }
  .content {
    margin: 0; }
  h1, h2 {
    margin: 50px 0;
    padding: 15px; }
  h3 {
    margin: 10px 0 10px 20px; }
  .cover h2, .requirements h2, .worksheet h2, .lesson h2 {
    padding: 0; }
  .lessons nav.buttons h2 {
    margin: 20px;
    line-height: 30px; }
  .content p, .content ol, .content ul {
    margin: 20px; }
  .content ol li, .content ul li {
    padding: 0px; }
  ul.hardware-items {
    width: 800px;
    margin: 0 auto; }
  .content pre {
    width: 92%;
    margin: 20px 2%;
    padding: 10px 2%; }
  nav.buttons {
    margin: 0;
    border-top: 0; }
  footer.footer {
    margin: 20px 10px 0px;
    padding: 0px 0px 20px;
    text-align: center; }
  footer.footer a.cc, footer.footer a.gh {
    float: none;
    margin: 10px auto; }
  footer.footer small {
    float: none;
    margin: 0;
    padding: 0; } }

