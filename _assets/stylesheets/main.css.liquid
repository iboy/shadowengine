.container {
  width: 900px;
  margin: 0 auto;
  border-width: 0 5px;
  border-style: solid;
  border-color: #438aa5; }

.container-grid {
  width: 1024px;
  margin: 0 auto;
  background: #e6e2dd; }

.container-grid {
  overflow: hidden; }

header.header {
  width: 100%;
  height: 60px;
  background: #438aa5 url("{{ site.baseurl }}{% asset_path logo.png %}") no-repeat;
  overflow: auto;
  font-family: "Roboto Slab"; }

header.header a.heading {
  display: block;
  color: #FFF;
  font-size: 18px;
  font-weight: 700;
  text-decoration: none;
  width: 100%;
  height: 100%;
  text-indent: 60px;
  line-height: 60px; }

nav.nav ul li a {
  color: #438aa5;
  text-decoration: none; }

nav.nav ul li a:hover {
  text-decoration: underline; }

nav.breadcrumbs {
  float: left; }

nav.breadcrumbs ul {
  margin: 10px 0 0 10px;
  padding: 0; }

nav.breadcrumbs ul li {
  float: left;
  list-style-type: none;
  font-size: 10px; }

nav.breadcrumbs ul li + li:before {
  float: left;
  width: 20px;
  content: ">";
  text-align: center; }

nav.resource-categories {
  float: right;
  margin: 10px 20px 30px; }

nav.resource-categories ul {
  margin: 0;
  padding: 0;
  overflow: auto; }

nav.resource-categories ul li {
  display: block;
  float: left;
  list-style-type: none;
  padding: 0;
  font-family: "Roboto Slab";
  font-size: 18px;
  font-weight: 500;
  color: #438aa5; }

.help nav.resource-categories ul li.help, .teach nav.resource-categories ul li.teach, .learn nav.resource-categories ul li.learn, .make nav.resource-categories ul li.make {
  font-weight: 700; }

nav.resource-categories ul li + li:before {
  content: "/";
  margin: 10px; }

.main {
  margin: 60px auto 0;
  clear: both; }

.cover .cover-image {
  text-align: center; }

nav.buttons {
  margin: 50px 0;
  border-top: 4px solid #438aa5; }

nav.buttons a.button {
  display: block;
  float: left;
  width: 300px;
  height: 60px;
  line-height: 60px;
  margin: 50px;
  background: #438aa5;
  color: white;
  font-family: "Roboto Slab";
  font-size: 18px;
  font-weight: 700;
  text-align: center;
  text-decoration: none; }

nav.buttons.one a.button {
  margin: 50px auto;
  float: none; }

.lessons nav.buttons {
